{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "TemplatesLocation": {
            "type": "string",
            "defaultValue": "https://raw.githubusercontent.com/fredgis/AIRobot/main/ARM%20Templates"
        },
        "TemplatesLocationAccessToken": {
            "type": "securestring"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "name": "Network",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('TemplatesLocation'), '/deployment-network.json', '?token=', parameters('TemplatesLocationAccessToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                }
            }
        }
    ],
    "outputs": {}
}